{
  "containerDefinitions": [
      {
          "name": "rails",
          "image": "511837944532.dkr.ecr.ap-northeast-1.amazonaws.com/cost-men",
          "cpu": 0,
          "portMappings": [
              {
                  "name": "rails-3000-tcp",
                  "containerPort": 3000,
                  "hostPort": 3000,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "entryPoint": [
              "/usr/bin/entrypoint.sh"
          ],
          "command": [
              "rails",
              "server",
              "-b",
              "0.0.0.0",
              "-e",
              "production"
          ],
          "environment": [
            {
                "name": "RAILS_ENV",
                "value": "production"
            }
          ],
          "secrets": [
            {
              "name": "DATABASE_USERNAME",
              "valueFrom": "arn:aws:ssm:ap-northeast-1:511837944532:parameter/myapp/database/username"
            },
            {
              "name": "DATABASE_NAME",
              "valueFrom": "arn:aws:ssm:ap-northeast-1:511837944532:parameter/myapp/database/name"
            },
            {
              "name": "RAILS_MASTER_KEY",
              "valueFrom": "arn:aws:ssm:ap-northeast-1:511837944532:parameter/myapp/database/key"
            },
            {
              "name": "DATABASE_HOST",
              "valueFrom": "arn:aws:ssm:ap-northeast-1:511837944532:parameter/myapp/database/host"
            },
            {
              "name": "DATABASE_PASSWORD",
              "valueFrom": "arn:aws:ssm:ap-northeast-1:511837944532:parameter/myapp/database/password"
            }
          ],
          "environmentFiles": [],
          "mountPoints": [],
          "volumesFrom": [],
          "workingDirectory": "/cost_men",
          "ulimits": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "/ecs/cost-men",
                  "awslogs-region": "ap-northeast-1",
                  "awslogs-stream-prefix": "ecs"
              }
          }
      }
  ],
  "family": "cost-men",
  "taskRoleArn": "arn:aws:iam::511837944532:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::511837944532:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  },
  "tags": []
}